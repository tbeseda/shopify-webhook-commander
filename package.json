{
  "name": "shopify-webhook-commander",
  "description": "Set up Shopify webhook destinations from the command line",
  "version": "0.1.0",
  "author": "Taylor Beseda @tbeseda",
  "bin": {
    "shopify-webhook-commander": "./bin/run"
  },
  "bugs": "https://github.com/tbeseda/shopify-webhook-commander/issues",
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "cli-ux": "^5.6.3",
    "cross-fetch": "^3.1.4",
    "fs-extra": "^10.0.0",
    "graphql": "^15.5.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^5",
    "@types/node": "^14",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chai": "^4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "prettier": "^2.3.2",
    "ts-node": "^8",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/tbeseda/shopify-webhook-commander",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "shopify-webhook-commander",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7
    }
  },
  "repository": "tbeseda/shopify-webhook-commander",
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
